;\n\n    const result = await tasksCollection.deleteOne({ _id: ObjectId(data) });\n\n    client.close();\n\n    res.status(201).json({ message: \"Task deleted\" });\n  }\n}\n\nexport default handler;\n"]},"metadata":{},"sourceType":"module"}